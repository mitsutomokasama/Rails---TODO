<!DOCTYPE html>
<html>
<head>
  <title>Nchak-タスク管理ツール</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <%= render 'shared/header' %>
    <div id="contents">
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %>
      <%= yield %>
    </div>
    <%= render 'shared/footer' %>
  </div>
</body>
</html>
